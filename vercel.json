{
  "version": 2,
  "builds": [
    {
      "src": "gym-backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/gym-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/gym-backend/server.js",
      "methods": ["GET"]
    }
  ]
}
